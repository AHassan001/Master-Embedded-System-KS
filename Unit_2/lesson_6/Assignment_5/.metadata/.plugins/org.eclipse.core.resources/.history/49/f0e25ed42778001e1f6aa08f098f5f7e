  #include "stdio.h"

union job{
	char name[32];
	float alary;
	int worker_no;

}u;

struct job1{
	char name[32];
	float alary;
	int worker_no;

}s;

void main()
{
	printf("size of union=%d",sizeof(u));
	printf("\nsize of structure=%d",sizeof(s));

}
